{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "settings": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/index_settings"
        },
        {
          "properties": {
            "index": {
              "$ref": "#/definitions/index_settings"
            },
            "analysis": {
              "type": "object",
              "properties": {
                "analyzer": {
                  "type": "object",
                  "defaultSnippets": [
                    {
                      "label": "add new analyzer",
                      "body": {
                        "${1:ANALYZER}": {}
                      }
                    }
                  ],
                  "patternProperties": {
                    "^.*$": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "anyOf": [
                            {
                              "type": "string",
                              "enum": [
                                "custom"
                              ]
                            },
                            {
                              "$ref": "#/definitions/analyzers"
                            }
                          ]
                        },
                        "char_filter": {
                          "type": "array",
                          "items": {
                            "type": "string",
                            "examples": [
                              "html_strip"
                            ]
                          }
                        },
                        "tokenizer": {
                          "$ref": "#/definitions/tokenizers"
                        },
                        "filter": {
                          "type": "array",
                          "items": {
                            "$ref": "#/definitions/token_filters"
                          }
                        },
                        "position_increment_gap": {
                          "type": "integer",
                          "default": 100
                        }
                      }
                    }
                  }
                },
                "normalizer": {
                  "type": "object",
                  "defaultSnippets": [
                    {
                      "label": "add new normalizer",
                      "body": {
                        "${1:NORMALIZER}": {}
                      }
                    }
                  ],
                  "patternProperties": {
                    "^.*$": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "examples": [
                            "custom"
                          ]
                        },
                        "char_filter": {
                          "type": "array",
                          "items": {
                            "type": "string",
                            "examples": [
                              "html_strip"
                            ]
                          }
                        },
                        "filter": {
                          "type": "array",
                          "items": {
                            "type": "string",
                            "examples": [
                              "arabic_normalization",
                              "asciifolding",
                              "bengali_normalization",
                              "cjk_width",
                              "decimal_digit",
                              "elision",
                              "german_normalization",
                              "hindi_normalization",
                              "indic_normalization",
                              "lowercase",
                              "persian_normalization",
                              "scandinavian_folding",
                              "serbian_normalization",
                              "sorani_normalization",
                              "uppercase"
                            ]
                          }
                        }
                      }
                    }
                  }
                },
                "tokenizer": {
                  "type": "object",
                  "defaultSnippets": [
                    {
                      "label": "add new tokenizer",
                      "body": {
                        "${1:TOKENIZER}": {}
                      }
                    }
                  ],
                  "patternProperties": {
                    "^.*$": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "$ref": "#/definitions/tokenizers"
                        }
                      }
                    }
                  }
                },
                "filter": {
                  "type": "object",
                  "defaultSnippets": [
                    {
                      "label": "add new filter",
                      "body": {
                        "${1:FILTER}": {}
                      }
                    }
                  ],
                  "patternProperties": {
                    "^.*$": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "$ref": "#/definitions/token_filters"
                        }
                      }
                    }
                  }
                },
                "char_filter": {
                  "type": "object",
                  "defaultSnippets": [
                    {
                      "label": "add new char filter",
                      "body": {
                        "${1:CHAR_FILTER}": {}
                      }
                    }
                  ],
                  "patternProperties": {
                    "^.*$": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "enum": [
                            "html_strip",
                            "mapping",
                            "pattern_replace"
                          ]
                        }
                      },
                      "allOf": [
                        {
                          "if": {
                            "properties": {
                              "type": {
                                "const": "html_strip"
                              }
                            }
                          },
                          "then": {
                            "properties": {
                              "escaped_tags": {
                                "type": "array",
                                "items": {
                                  "type": "string",
                                  "examples": [
                                    "b"
                                  ]
                                }
                              }
                            }
                          }
                        },
                        {
                          "if": {
                            "properties": {
                              "type": {
                                "const": "mapping"
                              }
                            }
                          },
                          "then": {
                            "properties": {
                              "mappings": {
                                "type": "array",
                                "items": {
                                  "type": "string",
                                  "examples": [
                                    ":) => _happy_"
                                  ]
                                }
                              },
                              "mappings_path": {
                                "type": "string",
                                "examples": [
                                  "/path/to/mappings_file"
                                ]
                              }
                            }
                          }
                        },
                        {
                          "if": {
                            "properties": {
                              "type": {
                                "const": "pattern_replace"
                              }
                            }
                          },
                          "then": {
                            "properties": {
                              "pattern": {
                                "type": "string",
                                "examples": [
                                  "[0-9]"
                                ]
                              },
                              "replacement": {
                                "type": "string",
                                "examples": [
                                  "-$1-"
                                ]
                              },
                              "flags": {
                                "type": "string",
                                "examples": [
                                  "CASE_INSENSITIVE|COMMENTS"
                                ]
                              }
                            }
                          }
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      ]
    },
    "mappings": {
      "type": "object",
      "properties": {
        "dynamic_templates": {
          "type": "array",
          "defaultSnippets": [
            {
              "label": "add new dynamic template",
              "body": [
                {
                  "${1:DYNAMIC_TEMPLATE}": {}
                }
              ]
            }
          ],
          "items": {
            "type": "object",
            "patternProperties": {
              "^.*$": {
                "type": "object",
                "properties": {
                  "match": {
                    "type": "string",
                    "default": "",
                    "examples": [
                      "long_*"
                    ]
                  },
                  "unmatch": {
                    "type": "string",
                    "default": "",
                    "examples": [
                      "*_text"
                    ]
                  },
                  "path_match": {
                    "type": "string",
                    "default": "",
                    "examples": [
                      "name.*"
                    ]
                  },
                  "path_unmatch": {
                    "type": "string",
                    "default": "",
                    "examples": [
                      "*.name"
                    ]
                  },
                  "match_pattern": {
                    "type": "string",
                    "default": "",
                    "examples": [
                      "regex"
                    ]
                  },
                  "match_mapping_type": {
                    "type": "string",
                    "enum": [
                      "boolean",
                      "date",
                      "double",
                      "long",
                      "object",
                      "string",
                      "*"
                    ]
                  },
                  "mapping": {
                    "$ref": "#/definitions/field_mapping"
                  },
                  "runtime": {
                    "$ref": "#/definitions/runtime_field_mapping"
                  }
                }
              }
            }
          }
        },
        "date_detection": {
          "type": "boolean",
          "default": true
        },
        "dynamic_date_formats": {
          "type": "array",
          "default": [
            "strict_date_optional_time",
            "yyyy/MM/dd HH:mm:ss Z||yyyy/MM/dd Z"
          ],
          "items": {
            "type": "string",
            "examples": [
              "strict_date",
              "epoch_millis",
              "yyyy-MM-dd"
            ]
          }
        },
        "numeric_detection": {
          "type": "boolean",
          "default": false
        },
        "dynamic": {
          "$ref": "#/definitions/dynamic"
        },
        "properties": {
          "type": "object",
          "$ref": "#/definitions/field",
          "defaultSnippets": [
            {
              "label": "add new property",
              "body": {
                "${1:PROPERTY}": {}
              }
            }
          ]
        },
        "runtime": {
          "type": "object",
          "$ref": "#/definitions/runtime_field",
          "description": "Since Elasticsearch 7.11",
          "defaultSnippets": [
            {
              "label": "add new runtime property",
              "body": {
                "${1:RUNTIME_PROPERTY}": {}
              }
            }
          ]
        },
        "_meta": {
          "type": "object"
        },
        "_source": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": true
            },
            "includes": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "excludes": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        },
        "_field_names": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": true
            }
          }
        },
        "_routing": {
          "type": "object",
          "properties": {
            "required": {
              "type": "boolean",
              "default": false
            }
          }
        }
      }
    },
    "aliases": {
      "type": "object",
      "defaultSnippets": [
        {
          "label": "add new alias",
          "body": {
            "${1:ALIAS}": {}
          }
        }
      ],
      "patternProperties": {
        "^.*$": {
          "type": "object",
          "properties": {
            "filter": {
              "type": "object"
            },
            "is_write_index": {
              "type": "boolean",
              "default": false
            },
            "is_hidden": {
              "type": "boolean",
              "default": false,
              "description": "Since Elasticsearch 7.12"
            },
            "routing": {
              "type": "string"
            },
            "index_routing": {
              "type": "string"
            },
            "search_routing": {
              "type": "string"
            }
          }
        }
      }
    },
    "field_mapping": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "examples": [
            "text",
            "keyword",
            "long",
            "integer",
            "short",
            "byte",
            "double",
            "float",
            "half_float",
            "scaled_float",
            "unsigned_long",
            "date",
            "date_nanos",
            "boolean",
            "binary",
            "integer_range",
            "float_range",
            "long_range",
            "double_range",
            "date_range",
            "ip_range",
            "object",
            "nested",
            "geo_point",
            "geo_shape",
            "point",
            "shape",
            "ip",
            "constant_keyword",
            "wildcard",
            "completion",
            "search_as_you_type",
            "token_count",
            "murmur3",
            "histogram",
            "annotated-text",
            "percolator",
            "join",
            "alias",
            "dense_vector",
            "sparse_vector",
            "rank_feature",
            "rank_features",
            "flattened",
            "version",
            "aggregate_metric_double",
            "match_only_text"
          ]
        },
        "fields": {
          "$ref": "#/definitions/field",
          "defaultSnippets": [
            {
              "label": "add new field",
              "body": {
                "${1:FIELD}": {}
              }
            }
          ]
        },
        "properties": {
          "$ref": "#/definitions/field",
          "defaultSnippets": [
            {
              "label": "add new property",
              "body": {
                "${1:PROPERTY}": {}
              }
            }
          ]
        },
        "analyzer": {
          "$ref": "#/definitions/analyzers"
        },
        "normalizer": {
          "type": "string"
        },
        "coerce": {
          "type": "boolean",
          "default": true
        },
        "copy_to": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string",
                "examples": [
                  "other_field"
                ]
              }
            },
            {
              "type": "string",
              "examples": [
                "other_field"
              ]
            }
          ],
          "default": []
        },
        "doc_values": {
          "type": "boolean",
          "default": true
        },
        "dynamic": {
          "$ref": "#/definitions/dynamic"
        },
        "enabled": {
          "type": "boolean",
          "default": true
        },
        "eager_global_ordinals": {
          "type": "boolean",
          "default": false
        },
        "fielddata": {
          "type": "boolean",
          "default": false
        },
        "format": {
          "type": "string",
          "examples": [
            "yyyy-MM-dd"
          ]
        },
        "ignore_above": {
          "type": "integer",
          "examples": [
            8191,
            32766
          ]
        },
        "ignore_malformed": {
          "type": "boolean",
          "default": false
        },
        "index": {
          "type": "boolean",
          "default": true
        },
        "index_options": {
          "type": "string",
          "enum": [
            "docs",
            "freqs",
            "positions",
            "offsets"
          ]
        },
        "index_phrases": {
          "type": "boolean",
          "default": false
        },
        "index_prefixes": {
          "type": "object",
          "properties": {
            "min_chars": {
              "type": "integer",
              "default": 2
            },
            "max_chars": {
              "type": "integer",
              "default": 2
            }
          }
        },
        "norms": {
          "type": "boolean",
          "default": true
        },
        "null_value": {
          "examples": [
            "NULL"
          ]
        },
        "position_increment_gap": {
          "type": "integer",
          "default": 100
        },
        "search_analyzer": {
          "$ref": "#/definitions/analyzers"
        },
        "search_quote_analyzer": {
          "$ref": "#/definitions/analyzers"
        },
        "similarity": {
          "type": "string",
          "enum": [
            "BM25",
            "classic",
            "boolean"
          ],
          "default": "BM25"
        },
        "store": {
          "type": "boolean",
          "default": false
        },
        "term_vector": {
          "type": "string",
          "enum": [
            "no",
            "yes",
            "with_positions",
            "with_offsets",
            "with_positions_offsets",
            "with_positions_payloads",
            "with_positions_offsets_payloads"
          ],
          "default": "no"
        },
        "path": {
          "type": "string",
          "examples": [
            "field_path"
          ]
        },
        "dims": {
          "type": "integer",
          "examples": [
            3
          ]
        },
        "boost": {
          "type": "number",
          "default": 1.0
        },
        "depth_limit": {
          "type": "integer",
          "default": 20
        },
        "split_queries_on_whitespace": {
          "type": "boolean",
          "default": false
        },
        "ignore_z_value": {
          "type": "boolean",
          "default": true
        },
        "orientation": {
          "type": "string",
          "examples": [
            "left",
            "right"
          ]
        },
        "relations": {
          "type": "object",
          "additionalProperties": true,
          "defaultSnippets": [
            {
              "label": "add new relation",
              "body": {
                "${1:PARENT}": "${2:CHILD}"
              }
            }
          ]
        },
        "meta": {
          "type": "object"
        },
        "include_in_parent": {
          "type": "boolean",
          "default": false,
          "description": "Since Elasticsearch 7.6"
        },
        "include_in_root": {
          "type": "boolean",
          "default": false,
          "description": "Since Elasticsearch 7.6"
        },
        "scaling_factor": {
          "type": "integer",
          "examples": [
            100
          ]
        },
        "positive_score_impact": {
          "type": "boolean",
          "default": true
        },
        "max_shingle_size": {
          "type": "integer",
          "default": 3
        },
        "enable_position_increments": {
          "type": "boolean",
          "default": true
        },
        "script": {
          "type": "object",
          "description": "Since Elasticsearch 7.13",
          "properties": {
            "source": {
              "type": "string"
            }
          }
        },
        "on_script_error": {
          "type": "string",
          "examples": [
            "fail",
            "continue"
          ],
          "default": "fail",
          "description": "Since Elasticsearch 7.13"
        },
        "time_series_dimension": {
          "type": "boolean",
          "default": false,
          "description": "Since Elasticsearch 7.16"
        },
        "time_series_metric": {
          "type": "string",
          "examples": [
            "gauge",
            "counter",
            "histogram",
            "summary"
          ],
          "description": "Since Elasticsearch 7.16"
        }
      }
    },
    "runtime_field_mapping": {
      "type": "object",
      "description": "Since Elasticsearch 7.11",
      "properties": {
        "type": {
          "type": "string",
          "examples": [
            "boolean",
            "date",
            "double",
            "geo_point",
            "ip",
            "keyword",
            "long"
          ]
        },
        "format": {
          "type": "string",
          "examples": [
            "yyyy-MM-dd"
          ]
        },
        "script": {
          "type": "object",
          "properties": {
            "source": {
              "type": "string",
              "examples": [
                ""
              ]
            }
          }
        }
      }
    },
    "field": {
      "patternProperties": {
        "^.*$": {
          "$ref": "#/definitions/field_mapping"
        }
      }
    },
    "runtime_field": {
      "patternProperties": {
        "^.*$": {
          "$ref": "#/definitions/runtime_field_mapping"
        }
      }
    },
    "dynamic": {
      "$comment": "disable/enable dynamic field mapping at index level and field level",
      "default": true,
      "anyOf": [
        {
          "type": "string",
          "enum": [
            "strict",
            "runtime"
          ]
        },
        {
          "type": "boolean"
        }
      ]
    },
    "index_settings": {
      "type": "object",
      "properties": {
        "number_of_replicas": {
          "type": "integer",
          "default": 1
        },
        "number_of_shards": {
          "type": "integer",
          "default": 1
        },
        "auto_expand_replicas": {
          "type": "string",
          "default": "false",
          "examples": [
            "0-1"
          ]
        },
        "allocation.max_retries": {
          "type": "integer",
          "default": 5
        },
        "allocation.existing_shards_allocator": {
          "type": "string",
          "default": "gateway_allocator",
          "description": "Since Elasticsearch 7.9"
        },
        "analyze.max_token_count": {
          "type": "integer",
          "default": 10000
        },
        "blocks.metadata": {
          "type": "boolean",
          "default": false
        },
        "blocks.read": {
          "type": "boolean",
          "default": false
        },
        "blocks.read_only": {
          "type": "boolean",
          "default": false
        },
        "blocks.read_only_allow_delete": {
          "type": "boolean",
          "default": false
        },
        "blocks.write": {
          "type": "boolean",
          "default": false
        },
        "codec": {
          "type": "string",
          "default": "default"
        },
        "compound_format": {
          "type": "string",
          "default": "0.1"
        },
        "data_path": {
          "type": "string",
          "default": ""
        },
        "default_pipeline": {
          "type": "string",
          "default": "_none"
        },
        "fielddata.cache": {
          "type": "string",
          "default": "node"
        },
        "final_pipeline": {
          "type": "string",
          "default": "_none",
          "description": "Since Elasticsearch 7.5"
        },
        "flush_after_merge": {
          "type": "string",
          "default": "512mb"
        },
        "force_memory_term_dictionary": {
          "type": "string",
          "default": "false"
        },
        "format": {
          "type": "string",
          "default": "0"
        },
        "frozen": {
          "type": "string",
          "default": "false"
        },
        "gc_deletes": {
          "type": "string",
          "default": "60s"
        },
        "hidden": {
          "type": "boolean",
          "default": false,
          "description": "Since Elasticsearch 7.9"
        },
        "highlight.max_analyzed_offset": {
          "type": "string",
          "default": "1000000"
        },
        "indexing.slowlog.level": {
          "type": "string",
          "default": "TRACE"
        },
        "indexing.slowlog.reformat": {
          "type": "string",
          "default": "true"
        },
        "indexing.slowlog.source": {
          "type": "string",
          "default": "1000"
        },
        "indexing.slowlog.threshold.index.debug": {
          "type": "string",
          "default": "-1"
        },
        "indexing.slowlog.threshold.index.info": {
          "type": "string",
          "default": "-1"
        },
        "indexing.slowlog.threshold.index.trace": {
          "type": "string",
          "default": "-1"
        },
        "indexing.slowlog.threshold.index.warn": {
          "type": "string",
          "default": "-1"
        },
        "lifecycle.indexing_complete": {
          "type": "string",
          "default": "false"
        },
        "lifecycle.name": {
          "type": "string",
          "default": ""
        },
        "lifecycle.origination_date": {
          "type": "string",
          "default": "-1",
          "description": "Since Elasticsearch 7.5"
        },
        "lifecycle.parse_origination_date": {
          "type": "string",
          "default": "false",
          "description": "Since Elasticsearch 7.5"
        },
        "lifecycle.rollover_alias": {
          "type": "string",
          "default": ""
        },
        "lifecycle.step.wait_time_threshold": {
          "type": "string",
          "default": "12h",
          "description": "Since Elasticsearch 7.13"
        },
        "load_fixed_bitset_filters_eagerly": {
          "type": "string",
          "default": "true"
        },
        "mapper.dynamic": {
          "type": "string",
          "default": "true"
        },
        "mapping.coerce": {
          "type": "string",
          "default": "false"
        },
        "mapping.depth.limit": {
          "type": "string",
          "default": "20"
        },
        "mapping.dimension_fields.limit": {
          "type": "string",
          "default": "16",
          "description": "Since Elasticsearch 7.15"
        },
        "mapping.field_name_length.limit": {
          "type": "string",
          "default": "9223372036854775807"
        },
        "mapping.ignore_malformed": {
          "type": "string",
          "default": "false"
        },
        "mapping.nested_fields.limit": {
          "type": "string",
          "default": "50"
